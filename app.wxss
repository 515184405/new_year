/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
page{
  height:100%;
  overflow:hidden;
  position:fixed;
  left:0;
  top:0;
  width:100%;
}
.btn{
  padding:18rpx 46rpx;
  color:#fff;
  margin-bottom:40rpx;
  font-size: 32rpx;
  border-radius:5px;
  border:1px solid #ffde03;
  box-shadow: 0px 0px 10px #ffde03; 
}
.btn-link1{
  color:#ffde03;
}
.btn-link2{
  background-color:#ffde03;
  color:#ff2a08;
  font-weight: bold;
  font-family: '微软雅黑';
}
.other-navigator-hover{
  border-color:#fed100;
  background-color:#fed100;
}
.flex{
  display: flex;
  align-items: center;
  justify-content: center;
}